{
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.12.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "name": "mini-ecommerce-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --project ./",
    "_start": "node ./www/server",
    "start": "cross-env NODE_ENV=production npm-run-all -s build _start",
    "dev": "cross-env NODE_ENV=development concurrently \"npx tsc --watch\" \"nodemon -q www/server.js\"",
    "dev2": "cross-env NODE_ENV=development npm-run-all -s build _start",
    "migrations:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/db/data-source.ts",
    "migrations:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/db/data-source.ts",
    "migrations:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/db/data-source.ts ./src/migrations/%npm_config_name% -p",
    "migrations:script": "npx ts-node ./src/db/generate-sql-script"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@config": "www/config",
    "@controllers": "www/controllers",
    "@entities": "www/entities",
    "@data": "www/data",
    "@db": "www/db",
    "@dtos": "www/dtos",
    "@helpers": "www/helpers",
    "@mappers": "www/mappers",
    "@middlewares": "www/middlewares",
    "@models": "www/models",
    "@repos": "www/repos",
    "@routes": "www/routes",
    "@services": "www/services"
  }
}
